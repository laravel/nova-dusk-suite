{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.6.*",
        "laravel/nova": "*",
        "laravel/scout": "^4.0",
        "laravel/tinker": "^1.0",
        "otwell/custom-field": "*",
        "otwell/resource-tool": "*",
        "otwell/sidebar-tool": "*"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.19.10",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^3.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "test": "php artisan dusk --printer='Codedungeon\\PHPUnitPrettyResultPrinter\\Printer'",
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./../nova-lib"
        },
        {
            "type": "path",
            "url": "./nova-components/SidebarTool"
        },
        {
            "type": "path",
            "url": "./nova-components/ResourceTool"
        },
        {
            "type": "path",
            "url": "./nova-components/CustomField"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
