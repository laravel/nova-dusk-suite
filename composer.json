{
    "name": "laravel/nova-dusk-suite",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel",
        "dev"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "algolia/algoliasearch-client-php": "^3.3",
        "doctrine/dbal": "^3.5.1",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "laravel/dusk": "^8.1",
        "laravel/framework": "^10.48.4",
        "laravel/scout": "^10.0.1",
        "laravel/tinker": "^2.7",
        "laravel/vapor-core": "^2.22",
        "league/flysystem-aws-s3-v3": "^3.0",
        "meilisearch/meilisearch-php": "^1.1",
        "nova-kit/nova-on-vapor": "^2.0",
        "orchestra/testbench-core": "^8.24.4",
        "spatie/laravel-ray": "^1.17"
    },
    "require-dev": {
        "larastan/larastan": "^2.5.1",
        "laravel/breeze": "^1.1",
        "laravel/nova": "@dev",
        "laravel/pint": "^1.16",
        "nunomaduro/collision": "^7.5",
        "orchestra/testbench-dusk": "^8.24.1",
        "orchestra/workbench": "^8.0",
        "phpunit/phpunit": "^10.5"
    },
    "conflict": {
        "laravel/dusk": "7.12.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "vendor/laravel/nova/workbench/database/factories/",
            "Database\\Seeders\\": "vendor/laravel/nova/workbench/database/seeders/",
            "NovaComponents\\CustomField\\": "vendor/laravel/nova/workbench/nova-components/CustomField/src/",
            "NovaComponents\\IconsViewer\\": "vendor/laravel/nova/workbench/nova-components/IconsViewer/src/",
            "NovaComponents\\RememberTokenCopier\\": "vendor/laravel/nova/workbench/nova-components/RememberTokenCopier/src/",
            "NovaComponents\\ResourceTool\\": "vendor/laravel/nova/workbench/nova-components/ResourceTool/src/",
            "NovaComponents\\SidebarTool\\": "vendor/laravel/nova/workbench/nova-components/SidebarTool/src/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/laravel/nova/workbench/app",
            "Laravel\\Nova\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "11.5.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan nova:publish --force"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
