{
    "name": "otwell/resource-tool",
    "description": "A Laravel Nova resource tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laravel/nova": "*"
    },
    "autoload": {
        "psr-4": {
            "Otwell\\ResourceTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Otwell\\ResourceTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        { "type": "path", "url": "../../nova" }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
