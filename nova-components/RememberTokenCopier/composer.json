{
    "name": "otwell/remember-token-copier",
    "description": "A Laravel Nova asset.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laravel/nova": "*",
        "laravel/nova-devtool": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Otwell\\RememberTokenCopier\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Otwell\\RememberTokenCopier\\AssetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../nova"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laravel/nova-devtool"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
