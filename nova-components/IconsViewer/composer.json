{
    "name": "otwell/icons-viewer",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "laravel/nova": "*",
        "orchestra/workbench": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Otwell\\IconsViewer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Otwell\\IconsViewer\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        { "type": "path", "url": "../../nova" }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
